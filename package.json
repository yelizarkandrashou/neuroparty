  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint .",
    "format": "prettier \"**/*.+(json|ts|tsx|scss)\" --write --list-different",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "check-all": "yarn lint && yarn test && yarn build && yarn check-format && yarn commitlint",
    "check-format": "prettier --check .",
    "commitlint": "commitlint --from=HEAD~1",
    "create-component": "plop --plopfile ./scripts/plop/create-component/createComponent.js",
    "create-function": "plop --plopfile ./scripts/plop/create-function/createFunction.js",
    "create-hook": "plop --plopfile ./scripts/plop/create-hook/createHook.js",
    "reinstall-node-modules": "bash ./scripts/reinstall-node-modules.sh",
    "prepare": "husky install ./.husky"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.0",
    "@commitlint/cli": "^13.2.0",
